{
    "preset": "laravel",
    "rules": {
        "new_with_parentheses": false,
        "not_operator_with_space": false,
        "not_operator_with_successor_space": false,
        "concat_space": {
            "spacing" : "one"
        },
        "control_structure_continuation_position": {
            "position": "next_line"
        },
        "method_argument_space": {
            "on_multiline": "ensure_fully_multiline"
        }
    }
}
